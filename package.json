{
  "name": "winston-discord-bot",
  "version": "1.0.0",
  "description": "Winston Discord Bot",
  "main": "index.js",
  "scripts": {
    "all": "npm run lint:fix && npm run build",
    "build": "tsc --build",
    "dev": "nodemon --exec ts-node src/index.ts",
    "docs": "npx typedoc --out docs src",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint --fix . --ext .ts",
    "start": "node ./lib/index",
    "test": "jest --coverage && rm -r coverage",
    "test:watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ansonfoong/AnimalCrossing.git"
  },
  "author": "Anson Foong",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ansonfoong/AnimalCrossing/issues"
  },
  "homepage": "https://github.com/ansonfoong/AnimalCrossing#readme",
  "devDependencies": {
    "@types/jest": "^25.2.1",
    "@types/ws": "^7.2.3",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-plugin-import": "^2.20.2",
    "husky": "^4.2.3",
    "jest": "^25.3.0",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.8.2",
    "typedoc": "^0.17.4",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "discord.js": "^12.1.1",
    "dotenv": "^8.2.0"
  }
}
